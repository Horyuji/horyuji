@font-face {
  font-family: "myfont";
  font-style: normal;
  font-weight: normal;
  src: url("../fonts/myfont.eot");
  src: url("../fonts/myfont.eot?#iefix") format("eot"),
       url("../fonts/myfont.woff") format("woff"),
       url("../fonts/myfont.ttf") format("truetype"),
       url("../fonts/myfont.svg#icon") format("svg");
  unicode-range:
    /** iconrange:start */
    U+EA01,
    U+EA02,
    /** iconrange:end */
}

[class^="icon-"], [class*=" icon-"] {
  font-family: "myfont";
  font-smoothing: antialiased | grayscale;
  font-style: normal;
  font-variant: normal;
  font-weight: normal;
  line-height: 1;
  speak: none;
  text-transform: none;

}

/** iconfont:start */
.icon-star::before { content: "\EA01"; }
.icon-play::before { content: "\EA02"; }
/** iconfont:end */
